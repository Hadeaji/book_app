<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Book-API</title>
  </head>
  <body>
      <header>
          <h1>Best Books!</h1>
      </header>
      <main>
          <h1>Search Results</h1>
        <% searchResults.forEach( book => { %>
            <img src="<%=book.image%>" alt="">
            <h3><%= book.title %></h3>
            <h3><%=book.authors%></h3>
            <h4><%=book.description%></h4>
        <% }) %>
    </main>
  </body>
</html>